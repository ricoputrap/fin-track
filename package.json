{
  "name": "fintrack",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:push": "bunx drizzle-kit push --config=drizzle.config.ts",
    "db:generate": "bunx drizzle-kit generate --config=drizzle.config.ts",
    "db:migrate": "bunx drizzle-kit migrate --config=drizzle.config.ts",
    "db:up": "bunx drizzle-kit up --config=drizzle.config.ts",
    "db:studio": "bunx drizzle-kit studio",
    "db:local": "turso dev -p 8081 --db-file src/db/local/local.db",
    "db:seed": "bun run src/db/seed.ts"
